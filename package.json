{
  "name": "svelte-template",
  "version": "0.1.0",
  "description": "template for svelte with esbuild and flask",
  "author": "ian castleden",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/arabidopsis/svelte-template.git"
  },
  "main": "src/main.js",
  "scripts": {
    "serve": "npx live-server --mount=/static:app/static index.html",
    "build": "node svelte-build/build.main.mjs",
    "production": "NODE_ENV=production node svelte-build/build.main.mjs",
    "check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings  --fail-on-hint"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^4.0.1",
    "@types/bootstrap": "^5.2.6",
    "@types/google.visualization": "^0.0.68",
    "@types/plotly.js": "^2.12.18",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.17",
    "esbuild-svelte": "^0.7.3",
    "nunjucks": "^3.2.4",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bootstrap5-tags": "^1.5.15",
    "notyf": "^3.10.0",
    "npm-check-updates": "^16.10.8"
  }
